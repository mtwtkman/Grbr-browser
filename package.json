{
  "name": "grbr-browser",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-packager . Grbr --platform=all --arch=x64 --electronVersion=1.8.2 --overwrite",
    "pack:mac": "electron-packager . Grbr --platform=darwin --arch=x64 --electronVersion=1.8.2 --overwrite",
    "pack:win": "electron-packager . Grbr --platform=win32 --arch=x64 --electronVersion=1.8.2 --overwrite; node windowsInstaller.js; rm -rf ./Grbr-win32-x64",
    "pack:linux": "electron-packager . Grbr --platform=linux --arch=x64 --electronVersion=1.8.2 --overwrite"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "asar": "^0.14.2",
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "electron-winstaller": "^2.6.4"
  }
}
